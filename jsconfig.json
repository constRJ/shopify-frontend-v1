{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@src/*": [
        "src"
      ],
      "@components/*": [
        "src/components/*",
        "src/components"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@containers/*": [
        "src/containers/*"
      ],
      "@actions/*": [
        "src/actions/*"
      ],
      "@reducers/*": [
        "src/reducers/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@api/*": [
        "src/api/*"
      ],
      "@theme": [
        "src/styles/theme"
      ]
    },
    "module": "commonjs",
    "target": "es6"
  },
  "exclude": [
    "node_modules"
  ]
}